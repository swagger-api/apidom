{
  "name": "apidom-webpack-demo",
  "version": "0.1.0",
  "description": "ApiDOM demo demonstrating webpack usage of ApiDOM build fragments",
  "main": "index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=browser webpack --config config/webpack/browser.config.js --progress && cp ./dist/apidom-demo.browser.js ./public && cp ./dist/*.wasm ./public",
    "clean": "rimraf ./dist ./public/apidom-demo.browser.js ./public/*.wasm",
    "start": "http-server -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swagger-api/apidom.git"
  },
  "author": "Vladimir Gorej",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/swagger-api/apidom/issues"
  },
  "homepage": "https://github.com/swagger-api/apidom#readme",
  "devDependencies": {
    "@babel/cli": "=7.8.4",
    "@babel/core": "=7.9.6",
    "@babel/plugin-proposal-class-properties": "=7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "=7.9.6",
    "@babel/plugin-transform-modules-commonjs": "=7.9.6",
    "@babel/plugin-transform-runtime": "=7.10.1",
    "@babel/preset-env": "=7.9.6",
    "@babel/preset-typescript": "=7.9.0",
    "@babel/register": "=7.10.1",
    "babel-loader": "=8.1.0",
    "core-js": "=3.6.5",
    "cross-env": "=7.0.2",
    "file-loader": "^6.0.0",
    "http-server": "^0.12.3",
    "regenerator-runtime": "=0.13.5",
    "rimraf": "=3.0.2",
    "terser-webpack-plugin": "=3.0.2",
    "webpack": "=4.43.0",
    "webpack-cli": "=3.3.11"
  }
}
