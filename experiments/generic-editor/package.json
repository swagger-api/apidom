{
  "name": "generic-editor",
  "version": "0.1.0",
  "private": true,
  "description": "Generic Editor plugin for SwaggerUI",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/generic-editor.browser.min.js",
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:cjs && npm run build:umd:browser",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir cjs",
    "build:umd:browser": "cross-env BABEL_ENV=browser webpack --config config/webpack/browser.config.js --progress",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "clean": "rimraf ./es ./cjs ./dist",
    "test": "run-s test:unit:coverage",
    "test:unit": "cross-env BABEL_ENV=cjs jest --runInBand --config ./config/jest/jest.unit.config.js",
    "test:unit:coverage": "cross-env BABEL_ENV=cjs jest --runInBand --config ./config/jest/jest.unit.coverage.config.js",
    "security-audit": "run-s -sc security-audit:prod security-audit:dev",
    "security-audit:prod": "npm-audit --production --only=prod --audit-level=low",
    "security-audit:dev": "npm-audit --only=dev --audit-level=moderate"
  },
  "author": "Vladimir Gorej",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "=7.12.1",
    "@babel/core": "=7.12.3",
    "@babel/plugin-proposal-class-properties": "=7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "=7.12.1",
    "@babel/plugin-transform-runtime": "=7.12.1",
    "@babel/preset-env": "=7.12.1",
    "@babel/register": "=7.12.1",
    "@char0n/npm-audit": "gist:2964395223d7943c10396f59df9a8ea0",
    "@commitlint/cli": "=11.0.0",
    "@commitlint/config-conventional": "=11.0.0",
    "babel-loader": "=8.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "core-js": "^3.7.0",
    "cross-env": "=7.0.2",
    "eslint": "=7.13.0",
    "eslint-config-airbnb-base": "=14.2.1",
    "eslint-config-prettier": "=6.15.0",
    "eslint-plugin-import": "=2.22.1",
    "eslint-plugin-prettier": "=3.1.4",
    "expect": "^26.6.2",
    "husky": "=4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "=10.5.1",
    "lodash-webpack-plugin": "=0.11.5",
    "npm-run-all": "^4.1.5",
    "prettier": "=2.1.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "=3.0.2",
    "webpack": "=4.44.2",
    "webpack-cli": "=4.2.0"
  }
}
