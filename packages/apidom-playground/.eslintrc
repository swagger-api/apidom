{
  "extends": [
//    "react-app",
//    "react-app/jest",
//    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "globals": {
    "window": "writable"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "jsx-a11y",
    "eslint-plugin-prettier",
    "prettier"
  ],
  "rules": {
    "import/order": ["error", {
      "groups": [
        ["builtin", "external", "internal"],
        ["parent", "sibling", "index"]
      ],
      "newlines-between": "always"
    }],
    "prettier/prettier": "error",
    "import/no-unresolved": 0,
    "jsx-a11y/no-autofocus": [ 2, {
      "ignoreNonDOM": true
    }]
  }
}
