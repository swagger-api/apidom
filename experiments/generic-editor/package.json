{
  "name": "generic-editor",
  "description": "Generic Editor plugin for SwaggerUI",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "preinstall": "node ./scripts/file-exists.js apidom-built && exit 0 || touch apidom-built && cd ../../apidom && npm i && npm run build",
    "start": "env-cmd -f ./.env.development node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --config config/jest/jest.config.js",
    "test:cy:open": "env-cmd -f ./.env.development cypress open",
    "test:cy:run": "env-cmd -f ./.env.development cypress run",
    "test:cy:ci": "REACT_APP_CYPRESS_MODE=headless env-cmd -f ./.env.test node scripts/test-cypress.js",
    "test:cy:dev": "REACT_APP_CYPRESS_MODE=interactive env-cmd -f ./.env.test node scripts/test-cypress.js",
    "lint": "eslint ./ --ext .jsx",
    "lint:fix": "eslint ./ --ext .jsx --fix",
    "clean": "rimraf ./build",
    "security-audit": "npm run security-audit:prod  && npm run security-audit:dev",
    "security-audit:prod": "npm-audit --production --only=prod --audit-level=low",
    "security-audit:dev": "npm-audit --only=dev --audit-level=moderate"
  },
  "dependencies": {
