{
  "name": "apidom-ns-openapi-3-1",
  "version": "0.1.0",
  "description": "Parser adapter for parsing JSON documents into AsyncAPI 2.x.y namespace.",
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/apidom-ns-openapi-3-1.browser.min.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run typescript:declaration && npm run build:es && npm run build:cjs && npm run build:umd:browser",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --extensions '.ts' --root-mode 'upward'",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir cjs --extensions '.ts' --root-mode 'upward'",
    "build:umd:browser": "cross-env BABEL_ENV=browser webpack --config config/webpack/browser.config.js --progress",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "clean": "rimraf ./es ./cjs ./dist ./types",
    "test": "cross-env BABEL_ENV=cjs mocha",
    "test:update-snapshots": "cross-env UPDATE_SNAPSHOT=1 BABEL_ENV=cjs mocha",
    "perf": "cross-env BABEL_ENV=cjs node ./test/perf/index.js",
    "perf:visitor-shortcut": "cross-env BABEL_ENV=cjs node ./test/perf/visitor-shortcut.js",
    "perf:refract": "cross-env BABEL_ENV=cjs node ./test/perf/refract.js",
    "typescript:check-types": "tsc --noEmit",
    "typescript:declaration": "tsc -p declaration.tsconfig.json",
    "prepack": "copyfiles -u 2 ../../NOTICE ."
  },
  "author": "Vladimir Gorej",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime-corejs3": "=7.15.4",
    "@types/ramda": "=0.27.45",
    "apidom": "*",
    "minim": "=0.23.8",
    "ramda": "=0.27.1",
    "ramda-adjunct": "=2.33.0",
    "stampit": "=4.3.2"
  },
  "files": [
    "cjs/",
    "dist/",
    "es/",
    "types/",
    "LICENSE",
    "NOTICE",
    "README.md",
    "CHANGELOG.md"
  ]
}
